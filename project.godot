config_version=5

[application]

config/name="ScareBnB"
config/tags=PackedStringArray("created_by_me")
run/main_scene="res://scenes/move_gym.tscn"
config/features=PackedStringArray("4.5")
config/icon="res://icon.png"
config/windows_native_icon="res://icon.ico"

[autoload]

Broadcast="*res://code/game/broadcast.gd"
DebugDraw2D="*res://addons/debugdraw2d/DebugDraw2D.gd"

[display]

window/size/viewport_width=3840
window/size/viewport_height=2160
window/size/mode=2
window/stretch/mode="viewport"
window/stretch/aspect="keep_height"
window/size/width=1920
window/size/height=1080

[editor_plugins]

enabled=PackedStringArray("res://addons/debugdraw2d/plugin.cfg", "res://addons/godot_sparrow/plugin.cfg", "res://addons/kenney-spritesheet/plugin.cfg", "res://addons/toggle-external-editor/plugin.cfg")

[global_group]

player="The Player's avatars"
snap_to_grid="Reposition this node to be in the middle of its closes grid position."

